        {
            "name": "Startup Registration",
            "description": "API routes for registering startups on the platform\n\nClass StartupRegistrationApiController",
            "item": [
                {
                    "name": "* Register a Startup",
                    "request": {
                        "url": {
                            "protocol": "http",
                            "host": "localhost",
                            "path": "api\/v1\/startup\/register",
                            "query": []
                        },
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application\/json"
                            },
                            {
                                "key": "Accept",
                                "value": "application\/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"company_name\": \"Jane Ventures\",\n    \"caption\": \"quisquam\",\n    \"product_image\": \"quis\",\n    \"funds_to_raise\": \"Ghc 234.00\",\n    \"duration_for_raise\": \"3 months.\",\n    \"startup_id\": 1\n}"
                        },
                        "description": "Registers a startup. First step out of 7 steps.\nThe same route is used to update the startup, if startup id is provided.\nWhen registration is done, a startup id is returned. This then can be used for the next stages.",
                        "response": []
                    }
                },
